--variables
function _init()
	poke(0x5f2d, 1)
	
	dx = 0
	dy = 0
	mouse={
		x,
		y,
		btns,
		startx,
		starty,
		inside=false,
		isdown=false,
		firstdown=true,
	}
	menu={
		x1=109,
		y1=16,
		x2=117,
		y2=25,
	}
	seed={
		x1=109,
		y1=menu.y1+5,
		x2=117,
		y2=menu.y2+5,
	}
	money={
	xy=0,
	yi=0,
	quantity=0;
	}
end
