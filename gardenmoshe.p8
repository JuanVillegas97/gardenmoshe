--update and draw
function _update60()
	mouse.x=stat(32)
	mouse.y=stat(33)
	mouse.btns=stat(34)
	mouse.sprite = mget(mouse.x/8,mouse.y/8)
	--here make loop to iterate with all the components
	mouse.inside=ismousein(menu)
	mouse.inside=ismousein(seed)
	
	
	if mouse.btns == 1 then
				mouse.isdown = true
	else
				mouse.isdown = false
				mouse.firstdown = true
	end


end

function _draw()
	cls()
	map(0,0)
	
	--please dont forget
	spr(71,menu.x1,menu.y1,1,4)
	
	if mouse.isdown and
				mouse.inside 
 then
 	if mouse.firstdown then
 		mouse.startx = mouse.x
 		mouse.starty = mouse.y
 		mouse.firstdown = false 
 	end
 	
 	dx = mouse.x-mouse.startx
 	dy = mouse.y-mouse.starty
 	
 	--ierate over all the objects
 	mousedragfing(menu,dx,dy)
 	
 	
 	mouse.startx = mouse.x
 	mouse.starty = mouse.y
		spr(3,mouse.x,mouse.y)--3
	elseif mouse.inside
	then
		spr(2,mouse.x,mouse.y)--2
	else
		spr(1,mouse.x,mouse.y)--1
	end
	
	print(mouse.x,12,70)
	print(mouse.y)
	print(mouse.btns)
	print(mouse.startx)
	print(mouse.starty)
	print(mouse.sprite)
	print(mouse.firstdown)
	print(mouse.isdown)
	
	print("random name",12,10,9)
	print(money.quantity,78,10,0)
	print("❎",84,10,9)
	print(stat(94),100,10,0)
	print(":",107,10,0)
	print(stat(95),110,10,0)
end




