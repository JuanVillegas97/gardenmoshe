--mouse
function ismousein(obj)
	if mouse.x > obj.x1 and
				mouse.x < obj.x2 and
				mouse.y > obj.y1 and
				mouse.y < obj.y2 
	then
				return true
	else
				return false
	end
end

function mousedragfing(obj,dx,dy)
		obj.x1=obj.x1+dx	
 	obj.y1=obj.y1+dy
 	obj.x2=obj.x2+dx	
 	obj.y2=obj.y2+dy
end
